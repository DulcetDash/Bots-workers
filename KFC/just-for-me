<script>
    function GetDeviceType() {
        if (navigator.userAgent.match(/Android/i)
            || navigator.userAgent.match(/webOS/i)
            || navigator.userAgent.match(/iPhone/i)
            || navigator.userAgent.match(/iPad/i)
            || navigator.userAgent.match(/iPod/i)
            || navigator.userAgent.match(/BlackBerry/i)
            || navigator.userAgent.match(/Windows Phone/i)
        ) {
            return "Mobile";
        }
        else {
            return "Desktop";
        }
    }
    var deviceType = GetDeviceType();
    var orderSource = 'False' == 'True' ? '' : deviceType;
    var currentLocation = window.location.href;

    if ('None' != 'None' && 'None' =='Pickup') {
        dataLayer.push({
            'event': 'set-custom dimension',
            'cd_storeID': '',
            'cd_disposition': 'None',
            'cd_userType': 'Guest',
            'language': 'en-US',
            'cd_futureOrder': 'True',
            'cd_reorder': 'False',
            'favouriteOrder': 'False',
            'countofproductssold': '',
            'requesttime': '',
            'deliveryinitialpromisetime': '',
            'deliveryfinalpromisetime': '',
            'cd_loggedIn': 'False',
            'cd_loggedInType': '',
            'voucherCode': '',
            
            'cd_paymentMethod': 'None',
            'rewardsAvailable': '',
            'cd_userID': '',
            'orderSource': orderSource,
            'CollectionPoint': 'None',
            'cd_postLocalisation': 'False',
            'cd_pageURL': currentLocation
        });
        dataLayer.push({ 'ecommerce': undefined });
    }
    else {
            dataLayer.push({
            'event': 'set-custom dimension',
            'cd_storeID': '',
            'cd_disposition': 'None',
            'cd_userType': 'Guest',
            'language': 'en-US',
            'cd_futureOrder': 'True',
            'cd_reorder': 'False',
            'favouriteOrder': 'False',
            'countofproductssold': '',
            'requesttime': '',
            'deliveryinitialpromisetime': '',
            'deliveryfinalpromisetime': '',
            'cd_loggedIn': 'False',
            'cd_loggedInType': '',
            'voucherCode': '',
            
            'cd_paymentMethod': 'None',
            'rewardsAvailable': '',
            'cd_userID': '',
            'orderSource': orderSource,
            'cd_postLocalisation': 'False',
            'cd_pageURL': currentLocation
        });
        dataLayer.push({ 'ecommerce': undefined });
    }
</script>
    
    
    
    
 

    
