<script>
    function GotoCartPage() {
         var hiddenValue = document.getElementById("hidRecommendPageShow");
         if (hiddenValue !== null && hiddenValue !== undefined) {
            var hidRecommendPageShow = document.getElementById("hidRecommendPageShow").value;
            //alert("hidRecommendPageShow " + hidRecommendPageShow);
            if (hidRecommendPageShow == "true") {
                window.location.href = "/menu/recommended";
            } else {
                window.location.href = "/menu/my-cart";
            }
        }else{
		   window.location.href = "/menu/my-cart";
		}
    }
     function OpenLink(link) {
        window.location.href = link;
    }
</script>
