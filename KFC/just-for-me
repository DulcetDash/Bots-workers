<script>
    //-------OSERV-17342----Start-------
    $('#failedAccessModal').on('shown.bs.modal', function () {
        pushDataLayerPopup('Delivery Not Available');
    });

    $('#riderUnavailableAccessModal').on('shown.bs.modal', function () {
        pushDataLayerPopup('Delivery - Rider Not Available');
    });

    $(document).on('click', '#findkfcbtn', function () {
        var self = $(this);
        var pushObj = {
            event: 'findStore',
            eventCategory: 'Find Store',
            eventLabel: 'GBL_ZA',
            eventAction: 'Click'
        };
        dataLayer.push(pushObj);
    })

    $(document).on('click', '.menu-main slick a', function () {
        var self = $(this);
        var pushObj = {
            event: 'bannerClick',
            eventCategory: 'Banner Interaction',
            eventLabel: 'GBL_ZA',
            eventAction: self.attr('href')//'document.url'
        };
        dataLayer.push(pushObj);
    })

    $(document).on('click', '.footer-content a', function () {
        var self = $(this);
        var devicename = GetDeviceName();
        if (devicename == "desktop") {
            var gtmEventAction = $(self).data('gtm-event-action');
            var pushObj = {
                event: 'footerInteraction',
                eventCategory: 'Footer',
                eventLabel: 'GBL_ZA',
                eventAction: gtmEventAction//'Item clicked'
            };
        } else {
            var gtmEventAction = $(self).data('gtm-event-action');
            var gtmEventLabel = $(self).data('gtm-event-label');
            var pushObj = {
                event: 'navigationPane',
                eventCategory: 'Nav Pane Interaction',
                eventLabel: 'GBL_ZA',
                eventAction: gtmEventAction
            };
        }
        dataLayer.push(pushObj);
    })
    //-------OSERV-17342----End-------


    $(document).on('click', '[data-gtm-click-event-tracker="true"]', function () {
        var self = $(this);
        //------OSERV-17342----Start----
        var gtmEventName = $(self).data('gtm-event-name');
        var gtmEventAction = $(self).data('gtm-event-action');
        var gtmEventLabel = $(self).data('gtm-event-label');
        var gtmEventCategory = $(self).data('gtm-event-category');
        var gtmcd_postLocalisation = $(self).data('gtm-event-cd_postLocalisation');
        var gtmcd_userType = $(self).data('gtm-event-cd_userType');
        var gtmcd_userType = $(self).data('gtm-event-cd_userType');
        var gtmcd_loggedIn = $(self).data('gtm-event-cd_loggedIn');
        var gtmcd_loggedInType = $(self).data('gtm-event-cd_loggedInType');
        var gtmcd_reorder = $(self).data('gtm-event-cd_reorder');

        var pushObj = {};
        pushObj.event = gtmEventName;
        pushObj.eventAction = gtmEventAction;
        if (gtmEventLabel) { pushObj.eventLabel = gtmEventLabel; }
        if (gtmEventCategory) { pushObj.eventCategory = gtmEventCategory; }
        if (gtmcd_postLocalisation) { pushObj.cd_postLocalisation = (gtmcd_postLocalisation == "true") ? true : false; }
        if (gtmcd_userType) { pushObj.cd_userType = gtmcd_userType; }
        if (gtmcd_loggedIn) { pushObj.cd_loggedIn = gtmcd_loggedIn; }
        if (gtmcd_loggedInType) { pushObj.cd_loggedInType = gtmcd_loggedInType; }
        if (gtmcd_reorder) { pushObj.cd_reorder = (gtmcd_reorder == "true") ? true : false; }


        if (gtmEventName) {
            try {
                if (gtmEventName == "navigationPane") {
                    if (document.URL.endsWith("/menu")) {
                        dataLayer.push(pushObj);
                        //console.log("dataLayer",dataLayer);
                    }
                } else {
                    dataLayer.push(pushObj);
                    //console.log("dataLayer",dataLayer);
                }
                //------OSERV-17342----End----

                //if (gtmEventLabel) {
                //    dataLayer.push({
                //        'event': gtmEventName,
                //        'eventAction': gtmEventAction,
                //        'eventLabel': gtmEventLabel
                //    });
                //}
                //else {
                //    dataLayer.push({
                //        'event': gtmEventName,
                //        'eventAction': gtmEventAction
                //    });
                //}
            }
            catch (err) {
            }
        }
    });
</script>



